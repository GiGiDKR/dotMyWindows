"Icon"="C:\\Users\\%USERNAME%\\AppData\\Local\\Programs\\PearAI\\PearAI.exe"

[HKEY_CLASSES_ROOT\*\shell\OpenWithPearAI\command]
@="\"C:\\Users\\%USERNAME%\\AppData\\Local\\Programs\\PearAI\\PearAI.exe\" \"%1\""

