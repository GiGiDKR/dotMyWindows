Windows Registry Editor Version 5.00

; Add to folder
[HKEY_CLASSES_ROOT\Directory\shell\OpenWithPearAI]
@="Ouvrir avec PearAI"
"Icon"="C:\\Users\\%USERNAME%\\AppData\\Local\\Programs\\PearAI\\PearAI.exe"

[HKEY_CLASSES_ROOT\Directory\shell\OpenWithPearAI\command]
@="\"C:\\Users\\%USERNAME%\\AppData\\Local\\Programs\\PearAI\\PearAI.exe\" \"%V\""

; Add to background folder
[HKEY_CLASSES_ROOT\Directory\Background\shell\OpenWithPearAI]
@="Ouvrir dans PearAI"
"Icon"="C:\\Users\\%USERNAME%\\AppData\\Local\\Programs\\PearAI\\PearAI.exe"

[HKEY_CLASSES_ROOT\Directory\Background\shell\OpenWithPearAI\command]
@="\"C:\\Users\\%USERNAME%\\AppData\\Local\\Programs\\PearAI\\PearAI.exe\" \"%V\""

; Add to all files
[HKEY_CLASSES_ROOT\*\shell\OpenWithPearAI]
